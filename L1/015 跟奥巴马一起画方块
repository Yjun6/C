#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>

int main()
{
	int i, j, c;
	int m;
	char n;
	printf("请输入您要输出的行和字符");
	scanf("%d %c", &m, &n);
	if (m % 2 == 1)
	{
		j = m / 2 + 1;
	}
	else
	{
		j = m / 2;
	}
	for (c = 0; c < j; c++)
	{
		for (i = 0; i < m; i++)
		{
			printf("%c", n);
		}
		printf("\n");
	}
	return 0;
}